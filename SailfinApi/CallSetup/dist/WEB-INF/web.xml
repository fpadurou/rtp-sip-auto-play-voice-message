<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>com.sun.faces.verifyObjects</param-name>
        <param-value>false</param-value>
    </context-param>
    <context-param>
        <param-name>com.sun.faces.validateXml</param-name>
        <param-value>true</param-value>
    </context-param>
    <context-param>
        <param-name>javax.faces.STATE_SAVING_METHOD</param-name>
        <param-value>client</param-value>
    </context-param>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
        </servlet>
    <servlet>
        <servlet-name>RegistrationBrowserServlet</servlet-name>
        <servlet-class>com.ericsson.sip.RegistrationBrowserServlet</servlet-class>
    </servlet>
     <servlet>         
        <servlet-name>SipCallSetupServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.SipCallSetupServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet>         
        <servlet-name>EndCallServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.EndCallServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet>         
        <servlet-name>HoldCallServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.HoldCallServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>	
     <servlet>         
        <servlet-name>LoginUserServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.auth.LoginUserServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet>         
        <servlet-name>LogoutUserServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.auth.LogoutUserServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>
     <servlet>         
        <servlet-name>CreateUserServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.auth.CreateUserServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>	
     <servlet>         
        <servlet-name>UtilServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.UtilServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>		
     <servlet>         
        <servlet-name>VoiceFlowServlet</servlet-name>      
        <servlet-class>com.ericsson.sip.VoiceFlowServlet</servlet-class>    
        <load-on-startup>1</load-on-startup>
    </servlet>		
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationBrowserServlet</servlet-name>
        <url-pattern>/RegistrationBrowserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>  
        <servlet-name>HoldCallServlet</servlet-name>
        <url-pattern>/HoldCallServlet</url-pattern>
    </servlet-mapping>	
    <servlet-mapping>  
        <servlet-name>EndCallServlet</servlet-name>
        <url-pattern>/EndCallServlet</url-pattern>
    </servlet-mapping>	
    <servlet-mapping>       
        <servlet-name>LoginUserServlet</servlet-name>
        <url-pattern>/LoginUserServlet</url-pattern>
    </servlet-mapping>	
    <servlet-mapping>       
        <servlet-name>LogoutUserServlet</servlet-name>
        <url-pattern>/LogoutUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>       
        <servlet-name>CreateUserServlet</servlet-name>
        <url-pattern>/CreateUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>       
        <servlet-name>UtilServlet</servlet-name>
        <url-pattern>/UtilServlet</url-pattern>
    </servlet-mapping>	
    <servlet-mapping>       
        <servlet-name>VoiceFlowServlet</servlet-name>
        <url-pattern>/VoiceFlowServlet</url-pattern>
    </servlet-mapping>	
    <servlet-mapping>       
        <servlet-name>SipCallSetupServlet</servlet-name>
        <url-pattern>/SipCallsetupServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
	<welcome-file>
            index.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
